
{
    "version": 2,
    "builds": [
      {
        "src": "package.json",  
        "use": "@vercel/static-build"
      },
      {
        "src": "server/server.cjs",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",  
        "dest": "/server/$1"
      },
      {
        "src": "/(.*)",  
        "dest": "/$1"
      }
    ]
  }
  